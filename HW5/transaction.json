{
    "request": {
        "urlPathPattern": "/transaction/",
        "queryParameters": {
            "price": {
                "matches": "[0-9]*"
            },
            "date": {
                "matches": "[0-9]{4}-[0-1][0-9]-[0-3][0-9]"
            }
        },
        "method": "GET"
    },
    "response": {
        "status": 200,
        "body": "Molodec!"
    }
}